
<%= notice %></br></br>

<%=form_for(@user) do |f|%>

<table>
<tr>
  <td><%= f.label "First Name" %></td>
  <td><%= f.text_field :name%></td>
  <td><%= f.label :password %></td>
  <td><%= f.text_field :password %></td>
</tr>
<tr>
  <td><%= f.label "Your Email Address" %></td>
  <td><%= f.text_field :email %></td>
  <td><%= f.label "Confirm your password" %></td>
  <td><%= f.text_field :password_confirmation %></td>
</tr>
</table>
  <%= f.submit "Create an Account" %>
<%end%>